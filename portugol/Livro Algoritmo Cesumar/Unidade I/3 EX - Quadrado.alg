algoritmo "Quadrado"
// Função :
// Autor :
// Data : 06/07/2019
// Seção de Declarações 
var
   L, A, P: real
inicio
      Escreva ("Informe o valor do lado do quadrado: ")
      Leia (L)
      A <- L * L
      P <- 4 * L
      Escreval ("O perimetro é: ", P)
      Escreval ("A área é: ", A)
fimalgoritmo
